package stream

import (
	"testing"
)

func TestStreamAPI(t *testing.T) {
	ints := []int{1, 2, 3, 4, 5}
	s := Of(ints)
	//s := Of[int](ints)
	if s == nil {
		t.Fail()
	}

	anyMatch := s.AnyMatch(func(e int) bool {
		return e == 3
	})

	if !anyMatch {
		t.Fatal("AnyMatch failed")
	}

	res, _ := s.FindAny(func(e int) bool {
		return e == 4
	})

	if res == 0 {
		t.Fatal("FindAny failed")
	}

	allMatch := s.AllMatch(func(e int) bool {
		return e > 0
	})
	if !allMatch {
		t.Fatal("AllMatch failed")
	}

	ns := s.ConcatArray([]int{1, 2, 3})

	if ns.Count() != 8 {
		t.Fatal("ConCatArray Failed")
	}

	ds := s.Distinct(func(e1, e2 int) int {
		return e1 - e2
	})

	if ds.Count() != 5 {
		t.Fatal("Distinct Failed")
	}

	ns = s.Filter(func(e int) bool {
		return e < 4
	})

	if ns.Count() != 3 {
		t.Fatal("filter failed")
	}

	s = Of(ints)

	max, _ := s.Max(func(e1, e2 int) int {
		return e1 - e2
	})

	if max != 5 {
		t.Fatal("Max failed")
	}
	min, _ := s.Min(func(e1, e2 int) int {
		return e1 - e2
	})

	if min != 1 {
		t.Fatal("Min failed")
	}
}

type Constraint1 interface {
	Method1()
}

type Constraint2 interface {
	Method2()
}

type Constraint3 interface {
	Constraint1
	Constraint2
}

